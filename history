    oc get pods
    2  oc get bc
    3  oc edit bc/hpapp
    4  oc get svc
    5  oc get route
    
    oc expose svc/myapp
    oc new-app https://github.com/vijkes/oc.git --name=myapp
    
    oc rsh <podname>
    oc run mypod --image=tomcat
    oc create deployment (deployment name) --image=(image name )
